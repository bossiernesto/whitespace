// Whitespace Assembler
[Assembler|source;ops;dest|assemble(file);translate(line);setUpFiles]
[Assembler]<-.-includes[TranslateTable]


//whitespace diagram
[Interpreter|stack;stackFrame;dumper;dissasembler;code|interpret();trace;dump()]
[Stack|stack|look(pos);dupTop();swapTop();copy(pos)]
[Interpreter]->[Stack]
[StackFrame|sym;retAddress;locals|getOutput()]
[FunctionSymbol|name;args;locals;addr|getOutput()]
[StackFrame]->[FunctionSymbol]
[Interpreter]++-0>[StackFrame]
[Dissasembler|disasembledCode|dissasemble(line)]
[Dumper||dumpStack();dumpStackTrace();dumpCode();dumpMemory()]
[Interpreter]->[Dumper]
[Interpreter]->[Dissasembler]
[Parser|InstructionsMap|+parse(IntructionReader);parse();readInstruction();getInstructionFactory()]
[Interpreter]<-[Parser]
[Instruction||execute()]
[SimpleInstruction]
[InstructionComposite]
[Instruction]^[SimpleInstruction]
[Instruction]^[InstructionComposite]
[InstructionComposite]++-0>[Instruction]
[Instruction]<->[Interpreter]
[Parser]++-1>[Instruction]
[CycleStartegy]
[Interpreter]<->[cycleStartegy]
[CycleStartegy]^[BurstCyCle]
[CycleStartegy]^[SingleCycle]
[CycleStartegy]->[Parser]